<div class="header-top">
  <div class="container">
    <div class="header-top-container">
      {% if page.header_top %}
        <div class="header-top-left">
            {{ page.header_top }}
        </div> <!--/.header-top-left -->
      {% endif %}
      {% if all_icons_show %}
        <div class="header-top-right">
          {% include '@tara/template-parts/social-icons.html.twig' %}
        </div> <!--/.header-top-right -->
      {% endif %}
    </div> <!--/.header-top-container -->
  </div> <!--/.container -->
</div> <!--/.header-top -->
